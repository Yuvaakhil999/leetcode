class Solution {
    public TreeNode bstFromPreorder(int[] preorder) {
        return ptb(preorder,Integer.MAX_VALUE,new int[]{0});
    }
    public TreeNode ptb(int[] nums,int bound,int[] i ){
        if(i[0]>=nums.length||nums[i[0]]>bound){
            return null;
        }
        TreeNode root=new TreeNode(nums[i[0]++]);
        root.left=ptb(nums,root.val,i);
        root.right=ptb(nums,bound,i);
        return root;
    }
}
